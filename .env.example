# Zotero API Configuration
# Copy this file to .env and fill in your credentials

# Your Zotero API key (get it from https://www.zotero.org/settings/keys/new)
ZOTERO_API_KEY=your_api_key_here

# Your Zotero user ID (found in your profile URL)
ZOTERO_USER_ID=your_user_id_here

# Library type: 'users' for personal library, 'groups' for group library
ZOTERO_LIBRARY_TYPE=users
